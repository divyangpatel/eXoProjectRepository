<?xml version="1.0" encoding="UTF-8"?>
<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd"
	version="1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd
                        http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd">
	<portlet>
		<portlet-name>PortletWithGeneric</portlet-name>
		<display-name>PortletWithGeneric</display-name>
		<portlet-class>com.portlet.UserPortlet</portlet-class>
		<init-param>
			<name>view-jsp</name>
			<value>/view.jsp</value>
		</init-param>
		<init-param>
			<name>help-jsp</name>
			<value>/help.jsp</value>
		</init-param>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>VIEW</portlet-mode>
			<portlet-mode>HELP</portlet-mode>
		</supports>
		<supported-locale>en</supported-locale>
		<portlet-info>
			<title>PortletWithGeneric</title>
			<short-title>PortletWithGeneric</short-title>
			<keywords>PortletWithGeneric</keywords>
		</portlet-info>
	</portlet>
</portlet-app>